      <AnchorPane prefHeight="150.0" prefWidth="373.0" />
      <ImageView fitHeight="400.0" fitWidth="605.0">
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="quizid" layoutX="197.0" layoutY="108.0" prefHeight="30.0" prefWidth="160.0" promptText="Quiz id">
         <font>
            <Font name="Fira Code SemiBold" size="14.0" />
         </font>
      </TextField>
      <Button fx:id="profile" layoutX="508.0" layoutY="32.0" mnemonicParsing="false" onAction="#onProButtonClick" style="-fx-background-color: #8D4559;" text="Profile">
